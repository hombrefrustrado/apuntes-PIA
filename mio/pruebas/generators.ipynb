   "source": [
    "def getfibo(n):\n",
    "    i1 = 1\n",
    "    i2 = 1\n",
    "    for i in range(n):\n",
    "        yield i2\n",
    "        temp = i2\n",
    "        i2 = i1\n",
    "        i1 = i1 + temp\n",
    "\n",
    "with open(\"fibo.txt\", \"w\") as f:\n",
    "    for a in getfibo(10000):\n",
    "        f.write(str(a) + \"\\n\")\n"
   ]
  }
